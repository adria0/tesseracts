{{ > header.handlebars }}

<table>
    <thead>
        <th>Block</th>
        <th>Transactions</th>
        <th>Timedate</th>
    </thead>
    <tbody>
        {{#each blocks}}
        <tr>
            <td><a href={{block.link}}>{{block.text}}</td>
            <td>{{tx_count}}</td>
            <td>{{timestamp}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

Last indexed block is #{{ last_indexed_block }}

